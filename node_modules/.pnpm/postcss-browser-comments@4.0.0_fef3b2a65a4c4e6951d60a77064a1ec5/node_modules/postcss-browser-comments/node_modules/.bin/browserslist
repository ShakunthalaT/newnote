#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/theia/notemaker/node_modules/.pnpm/browserslist@4.23.2/node_modules/browserslist/node_modules:/home/theia/notemaker/node_modules/.pnpm/browserslist@4.23.2/node_modules:/home/theia/notemaker/node_modules/.pnpm/node_modules:/home/theia/notemaker/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/notemaker/node_modules/.pnpm/postcss-browser-comments@4.0.0_fef3b2a65a4c4e6951d60a77064a1ec5/node_modules/postcss-browser-comments/node_modules:/home/theia/notemaker/node_modules/.pnpm/postcss-browser-comments@4.0.0_fef3b2a65a4c4e6951d60a77064a1ec5/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/theia/notemaker/node_modules/.pnpm/browserslist@4.23.2/node_modules/browserslist/node_modules:/home/theia/notemaker/node_modules/.pnpm/browserslist@4.23.2/node_modules:/home/theia/notemaker/node_modules/.pnpm/node_modules:/home/theia/notemaker/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/notemaker/node_modules/.pnpm/postcss-browser-comments@4.0.0_fef3b2a65a4c4e6951d60a77064a1ec5/node_modules/postcss-browser-comments/node_modules:/home/theia/notemaker/node_modules/.pnpm/postcss-browser-comments@4.0.0_fef3b2a65a4c4e6951d60a77064a1ec5/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../browserslist@4.23.2/node_modules/browserslist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../browserslist@4.23.2/node_modules/browserslist/cli.js" "$@"
fi
